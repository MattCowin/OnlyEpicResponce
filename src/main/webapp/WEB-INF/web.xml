<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>OnlyEpicRequest</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.revature.servlets.LoginServlet</servlet-class>
    <init-param>
      <param-name>LoginName</param-name>
      <param-value>LoginValue</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>AuthenticationFilter</display-name>
    <filter-name>AuthenticationFilter</filter-name>
    <filter-class>com.revature.filters.AuthenticationFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>AuthenticationFilter</filter-name>
    <url-pattern>/Users/userhome.html</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>JSession</display-name>
    <servlet-name>JSession</servlet-name>
    <servlet-class>com.revature.session.JSession</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>JSession</servlet-name>
    <url-pattern>/JSession</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>InvailedSession</display-name>
    <servlet-name>InvailedSession</servlet-name>
    <servlet-class>com.revature.session.InvailedSession</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InvailedSession</servlet-name>
    <url-pattern>/InvailedSession</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>FrontControllerServlet</display-name>
    <servlet-name>FrontControllerServlet</servlet-name>
    <servlet-class>com.revature.servlets.FrontControllerServlet</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>FrontControllerServlet</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <error-page>
    <error-code>404</error-code>
    <location>/somethingwentwrong.html</location>
  </error-page>
  <context-param>
    <param-name>someName</param-name>
    <param-value>someValue</param-value>
  </context-param>
</web-app>